{
  "hostvars":
  {
    "vagrant_image":         "elegoev/ubuntu-18.04-docker",
    "vagrant_ram":           "2048",
    "vagrant_cpu":           "2",
    "vb_hostname":           "docker",
    "vb_guest_os":           "ubuntu",
    "vb_group":              "vagrant-docker-fabric",
    "vb_dnshostresolve":     true,
    "vb_network": [
      {
        "ip":      "171.16.1.51",
        "netmask": "255.255.255.0",
        "netname": "vagrant-docker-fabric-net1",
        "nictype": "82540EM"
      }
    ],
    "vb_shell_provisioner": [
      {
        "script": "docker-jenkinsci-docker-fabric.sh"
      }
    ],
    "vb_port_forwarding": [
      {
        "host":         "8080",
        "host_ip":      "127.0.0.1",
        "guest":        "8080",
        "protocol":     "tcp",
        "id":           "http-jenkins"
      }
    ]
  }
}
